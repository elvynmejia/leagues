{
  "name": "api",
  "version": "1.0.0",
  "description": "api backend",
  "main": "backend.js",
  "scripts": {
    "tsc": "tsc",
    "postinstall": "npm run tsc",
    "build": "tsc",
    "dev": "tsc && node compiled/app/app.js",
    "prod": "node compiled/app/app.js",
    "test": "mocha -r ts-node/register spec/**/*.spec.ts --reporter spec --exit",
    "noto": "mocha -r ts-node/register spec/**/*.spec.ts --reporter spec --require spec/index.ts --no-timeout --exit",
    "lint": "eslint app/**/*.ts spec/**/*.spec.ts",
    "lint-fix": "eslint app/**/*.ts spec/**/*.spec.ts --fix"
  },
  "author": "Elvyn M",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.23.0",
    "cheerio": "^1.0.0-rc.10",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^8.47.0",
    "mocha": "^9.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
